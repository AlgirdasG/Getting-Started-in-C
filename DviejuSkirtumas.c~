#include <stdlib.h>
#include <stdio.h>

int main()

{

int a;
int b;
int diff;
printf("Si programa apskaiciuoja dvieju skaiciu skirtuma. \n");
printf("Iveskite pirmaji skaiciu: ");
scanf("%d", &a);
printf("Iveskite antraji skaiciu: ");
scanf("%d", &b);
diff = a - b;
printf("Dvieju skaiciu skirtumas yra: %d \n", diff);

}
