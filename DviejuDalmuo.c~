#include <stdlib.h>
#include <stdio.h>

int main()

{

int a;
int b;
int quotient;
printf("Si programa apskaiciuoja dvieju skaiciu dalmeni.\n");
printf("Iveskite pirmaji skaiciu: ");
scanf("%d", &a);
printf("Iveskite antraji skaiciu: ");
scanf("%d", &b);
quotient = a / b;
printf("Ivestu skaiciu dalmuo yra: %d\n", quotient);

}
